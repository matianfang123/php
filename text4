<?php
$info=array(
	'user'=>array(
		array(1,'zhangsan',10,'nan'),
		array(2,'lisi',12,'nv'),
		array(3,'wangwu',13,'nan'),
	),
	'score'=>array(
		array(1,100,90,80),
		array(2,20,43,60),
		array(3,54,67,21),
	),
	'connect'=>array(
		array(1,'110','aaa@bb.com'),
		array(2,'220','ccc@dd.com'),
		array(3,'330','eee@ff.com'),
	)
);
foreach($info as $k=>$v){
	echo "<table border=1 width=800 align=center>";
		echo"<caption><h1>".$k."</h1></caption>";
		foreach($v as $value){
			echo "<tr>";
			foreach($value as $o){
				echo "<td>";
				echo $o;
				echo "</td>";
			}
			
			echo "</tr>";
			
		}
	echo "</table>";
}





?>
